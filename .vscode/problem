class Add {
  constructor(...words) {
      this.words = words;
  }
  //your code goes here
  print(...words) {
    for (let t=0; t<words.length; t++) {
      words[t] = `$${words[t]}`
      console.log(words[t])
    }
  }
}

var x = new Add("hehe", "hoho", "haha", "hihi", "huhu");
var y = new Add("this", "is", "awesome");
var z = new Add("lorem", "ipsum", "dolor", "sit", "amet", "consectetur", "adipiscing", "elit");
x.print(x.words);
y.print(y.words);
z.print(z.words);